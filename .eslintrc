{
	"parser": "@typescript-eslint/parser",
	"parserOptions": { "ecmaVersion": "latest", "sourceType": "module" },
	"extends": ["plugin:@typescript-eslint/recommended"],
	"env": {
		"node": true
	},
	"rules": {
		"no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }],
		"no-console": "warn"
	}
}
